<h1>Listing product1s</h1>

<table>
  <tr>
    <th>\title</th>
    <th>Description</th>
    <th>Image url</th>
    <th>Price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product1s.each do |product1| %>
  <tr>
    <td><%= product1.\title %></td>
    <td><%= product1.description %></td>
    <td><%= product1.image_url %></td>
    <td><%= product1.price %></td>
    <td><%= link_to 'Show', product1 %></td>
    <td><%= link_to 'Edit', edit_product1_path(product1) %></td>
    <td><%= link_to 'Destroy', product1, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Product1', new_product1_path %>
